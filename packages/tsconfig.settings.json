{
  "compilerOptions": {
    "declarationMap": true,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "incremental": true,
    "allowJs": false,
    "target": "ESNext",
    "module": "ES6",
    "moduleResolution": "Node",
    "declaration": true,
    "composite": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@war-core/*": [
        "./war-core/src/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
